@model BindMultipleModel.Models.SchoolViewModel

<h1>Teachers Data in Table form --------------</h1>
<div class="container">
    <table class="table table-dark table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Qualiction</th>
                <th>Gender</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.Teachers)
            {
                <tr class="table-active">
                    <th>@item.Id</th>
                    <td>@item.name</td>
                    <td>@item.qualication</td>
                    <td>@item.gender</td>
                </tr>
            }
        </tbody>
    </table>
</div>